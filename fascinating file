class Solution {
public:
    bool isFascinating(int n) {
         string a=to_string(n*2);
	   string b=to_string(n*3);
	   string c=to_string(n);
	   string s=a+b+c;
	   int z=123456789;
	   string t=to_string(z);
	   sort(s.begin(),s.end());
	   if(s==t)
	   {
	       return true;
	   }
	   else
	   {
	       return false;
	   }
    }
};
